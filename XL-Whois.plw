      $winPb->lblPbCurr->Text('');
      $winPb->lblCount->Text('');
      $winPb->pbWinPb->SetPos(0);
      &winPb_Terminate;
      if ($confirm) {
        Win32::GUI::MessageBox($win, "$STR{'errDownloading'} $STR{'lblWSDB'}...", $STR{'Error'}, 0x40010);
      } else { return("$STR{'errDownloading'} $STR{'lblWSDB'}..."); }
    }
  } else {

#--------------------------#
{
  # Save the choice
  if (!$trayMenu->{'domainWhois'}->Checked()) {
    $trayMenu->{'domainWhois'}->Checked(1);
    $CONFIG{'OPT_DOMAIN'} = 1;
    $win->chDomain->Checked(1);
    &saveConfig(\%CONFIG);

      for (my $col = 0; $col <= $winDB->DBGrid->GetColumns(); $col++) {
        $content .= $winDB->DBGrid->GetCellText($row, $col) . "\t";
      }
      chop($content);
      $content .= "\r\n";
    }
  }
  Win32::Clipboard::Set($content) if $content;
  return(1);

}  #--- End lineCopy


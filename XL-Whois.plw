#--------------------------#
sub deleteISPGrid
#--------------------------#
{
  # Local variables
  my ($noRow, $ipStart, $ipEnd) = @_;
  # Find the ISP in grid and delete
  for (my $i = $noRow; $i < $winDB->DBGrid->GetRows(); $i++) {
    my $range_s = $winDB->DBGrid->GetCellText($i, 0);
    if ($ipStart eq $range_s) {

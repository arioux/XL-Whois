  if (open(my $IPv4_CSV, '<', $IPv4dbFile)) {
    while( my $line = <$IPv4_CSV>)  {
      if (!$first) { $first  = $line;       }
      else         { $second = $line; last; }
    }
    close $IPv4_CSV;
    return(1) if $first =~ /^Prefix,/ and $second =~ /^\d{3}\//;
  }
  return(0);

